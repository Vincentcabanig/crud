{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "@babel/runtime": "^7.1.5",
    "@blueprintjs/core": "^3.9.0",
    "@blueprintjs/icons": "^3.3.0",
    "axios": "^0.18.0",
    "connected-react-router": "^5.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "history": "^4.7.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "source-map-explorer": "^1.6.0",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.5",
    "eslint-config-airbnb": "^17.1.0",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug --fix src/",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
